# Backend API URL (server-side only, used by Next.js rewrites & API routes)
API_BASE_URL=http://localhost:8888/api

# Client-side API base (relative path, proxied through Next.js)
NEXT_PUBLIC_API_BASE_URL=/api

# WebSocket (direct connection to backend)
NEXT_PUBLIC_WS_URL=ws://localhost:8888/api/ws

# GitHub OAuth
NEXT_PUBLIC_GITHUB_CLIENT_ID=your-github-client-id
